# Authentication
app_password = "admin"

# External Storage: Google Sheets (Optional)
# Instructions:
# 1. Create Service Account in Google Cloud (see SERVICE_ACCOUNT_SETUP.md)
# 2. Key Type: "project_id" is automatically found in the JSON key file
# 3. Paste the fields from your JSON key here:
# [gcp_service_account]
# type = "service_account"
# project_id = "..."
# private_key_id = "..."
# private_key = "..."
# client_email = "..."
# client_id = "..."
# auth_uri = "..."
# token_uri = "..."
# auth_provider_x509_cert_url = "..."
# client_x509_cert_url = "..."

# Optional: Link to a specific sheet URL (otherwise it searches for 'ExpenseTracker_Data')
# spreadsheet_url = "https://docs.google.com/spreadsheets/d/..."
